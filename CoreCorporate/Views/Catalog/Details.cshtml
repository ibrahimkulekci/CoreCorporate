@model EntityLayer.Concrete.Catalog
@{
    ViewData["Title"] = Model.CatalogTitle;
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<section id="innerBanner">
    <div class="inner-content">
        <h2><span>@Model.CatalogTitle</span></h2>
        <div>
        </div>
    </div>
</section>

<main id="main">

    <!--==========================
      About Section
    ============================-->
    <section id="about" class="wow fadeInUp" style="visibility: visible; animation-name: fadeInUp;">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 content">
                    @Html.Raw(Model.CatalogContent)
                </div><br /><br /><br />
                <div class="col-lg-12 content">
                    @Html.Raw(Model.CatalogPdfIframe)
                </div>
                @if (Model.CatalogPdfIframe == null)
                {
                    <div class="col-lg-12 content">
                        <iframe src="@Model.CatalogPdfUrl" width="100%" height="500px">
                    </div>
                }
            </div>

        </div>
    </section><!-- #about -->

</main>
