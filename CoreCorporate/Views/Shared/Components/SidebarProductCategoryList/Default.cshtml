@model IEnumerable<EntityLayer.Concrete.ProductCategory>

<div class="sidebar_categories">
    <div class="theme_inner_title">
        <h4>Kategoriler</h4>
    </div>
    <ul class="list">
        @foreach (var item in Model)
        {
            <li><a href="~/Product/Category?id=@item.ProductCategoryID">@item.ProductCategoryTitle</a></li>
        }
    </ul>
</div>
